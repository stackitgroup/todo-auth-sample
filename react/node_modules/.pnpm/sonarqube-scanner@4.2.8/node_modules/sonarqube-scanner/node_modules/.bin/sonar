#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules/sonarqube-scanner/bin/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules/sonarqube-scanner/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules/sonarqube-scanner/bin/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules/sonarqube-scanner/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/sonarqube-scanner@4.2.8/node_modules:/Users/julianswerdlin/Desktop/sample/react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sonar-scanner.js" "$@"
else
  exec node  "$basedir/../../bin/sonar-scanner.js" "$@"
fi
